{
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-var": 2,
      "semi": [2, "always"],
      "indent": [2, 2, {
        "SwitchCase": 1
      }],
      "quotes": [2, "single"],
      "jsx-quotes": [2, "prefer-single"]
    }
}